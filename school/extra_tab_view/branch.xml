<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="student_branch_tree_view" model="ir.ui.view">
	    <field name="name">Student Branch Tree View</field>
	    <field name="model">student.branch</field>
	    <field name="arch" type="xml">
	        <tree string="Create Student Branch">
	            <field name="name"/>
	        </tree>
	    </field>
	</record>
	
	<record id="student_branch_form_view" model="ir.ui.view">
	    <field name="name">Student Branch Form View</field>
	    <field name="model">student.branch</field>
	    <field name="arch" type="xml">
	        <form string="Student Branch Form View">
	        	<sheet>
	        		<group col="4">
	        			<field name="name" class="oe_inline" />
	        		</group>
	        	</sheet>
	        </form>
	    </field>
	</record>
	
	<record model="ir.actions.act_window" id="student_branch_action_id">
	    <field name="name">Student Branch</field>
	    <field name="res_model">student.branch</field>
	    <field name="type">ir.actions.act_window</field>
	    <field name="view_type">form</field>
	    <field name="view_mode">tree,form</field>
	    <field name="view_id" eval="student_branch_tree_view" />
	</record>
	
	<menuitem id="menu_student_branch" name="Student Branch" parent="main_menu_student_extra_tab" action="student_branch_action_id" />
	
</odoo>










